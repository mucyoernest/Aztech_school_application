<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".Registrar">


    <TextView
        android:id="@+id/textView14"

        android:layout_width="338dp"
        android:layout_height="68dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/shadow3"
        android:gravity="center"
        android:onClick="openBrowser"
        android:orientation="horizontal"
        android:tag="https://docs.google.com/forms/d/e/1FAIpQLScvA2riFCKBnnqgm5PqXuB_l_AHdvz81KjR0541BF9SbEF4zg/viewform"
        android:text="Request For Unofficial Transcripts"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"

        android:layout_width="338dp"
        android:layout_height="68dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/shadow3"
        android:gravity="center"
        android:onClick="openBrowser"
        android:orientation="horizontal"
        android:tag="https://docs.google.com/forms/d/e/1FAIpQLScvA2riFCKBnnqgm5PqXuB_l_AHdvz81KjR0541BF9SbEF4zg/viewform"
        android:text="Letter of Support"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="104dp"
        android:text="Registrar's Office"
        android:textColor="#801414"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>