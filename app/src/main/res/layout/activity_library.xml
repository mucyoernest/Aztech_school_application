<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".library"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome to your Labrary"
        android:textSize="25dp"
        android:layout_marginTop="10dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="10dp"
        android:textColor="#131F56"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        >
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#315251"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView9"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:text="Art"
                android:textSize="16dp"
                android:textStyle="bold"
                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/art" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#4F6E71"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView4"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:text="Biology"
                android:textSize="16dp"
                android:textStyle="bold"

                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/bio" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#E85A56"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView16"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:text="cs"
                android:textSize="16dp"
                android:textStyle="bold"

                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/cs1" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        >
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#D6B983"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView17"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:text="Law"
                android:textSize="16dp"
                android:textStyle="bold"

                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/law" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#A86733"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView18"
                android:layout_width="130dp"
                android:textStyle="bold"

                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:text="Physics"
                android:textSize="16dp"
                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/physics" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#4C1C04"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView19"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textStyle="bold"

                android:textColor="@android:color/white"
                android:text="History"
                android:textSize="16dp"
                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/history" />

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        >
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#C87344"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView1"
                android:layout_width="130dp"
                android:textStyle="bold"

                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:text="Music"
                android:textSize="16dp"
                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/music" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#792517"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView3"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:text="Literature"
                android:textSize="16dp"
                android:textStyle="bold"

                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/literature" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="138dp"
            android:layout_height="170dp"
            android:orientation="vertical"
            android:background="#8B9BA6"
            android:paddingBottom="20dp"
            android:onClick="open_librairy"
            >
            <TextView
                android:id="@+id/textView"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:text="Math"
                android:textSize="16dp"
                android:textStyle="bold"

                android:layout_marginTop="20dp"
                android:fontFamily="serif-monospace"
                />
            <ImageView
                android:id="@+id/imageView"
                android:layout_width="90dp"
                android:layout_height="90dp"

                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"
                app:srcCompat="@drawable/math" />

        </LinearLayout>
    </LinearLayout>

</LinearLayout>